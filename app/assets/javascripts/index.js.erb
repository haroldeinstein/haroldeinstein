function preloadImage(image) {
  var img = new Image();
  img.src = image;
  img.onload = function() {
  };
}

$(document).ready(function() {
  var imageArr = [
    "<%= image_path "commercials_b_on.png" %>",
    "<%= image_path "underwear_b_on.png" %>"
  ];

  for (var i = 0; i < imageArr.length; i++) {
    preloadImage(imageArr[i]);
  }

  setTimeout(function() {
    $('#loading-outer').fadeOut(500, function() {
      $(this).hide();
    });
  }, 500)
});
